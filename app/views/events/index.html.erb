
<div class="container">
  <div class="jumbotron">
    <h1>Most Exited Events of 2018</h1> <br>

<table class="table table-striped table-inverse">
  <thead>
    <tr>
      <th>#</th>
      <th>Venue Id</th>
      <th>Band_Id</th>
      <th>Event Date</th>
      <th>Alcohol Served</th>
      <th>Edit-Cancel Event</th>
    </tr>
  </thead>
  <tbody>
  	<% @events.each do |t| %>
    <tr>
      <th scope="row">=></th>
      <td><%= t.venue_id %></td>
      <td><%= t.band_id %></td>
      <td><%= t.event_date %></td>
      <td><%= t.alcohol_served %></td>
      <td><%= link_to "Edit", edit_event_path(t) %> -
          <%= link_to "Remove", event_path(t), method: :delete, data: { confirm: "Are you sure?" } %>
    </tr>
    <% end %>
</tbody>
</table>
<%= link_to "Add New Event", new_event_path, class: 'btn btn-primary btn-block' %>
</div>
</div>
