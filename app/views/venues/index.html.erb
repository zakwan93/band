<div class ="container">
  <div id= "background" style="margin-bottom: 100px;">
    <img src = "my-band.jpg" height="100%" width="100%">
  </div>
</div>

<div class="container">
  <div class="jumbotron">
    <center>
        <h1>Venue for Events</h1> <br>
    </center>

              <table class="table table-striped table-inverse">
                <thead>
                  <tr>
                    <th>#</th>
                    <th>Venue Name</th>
                    <th>City</th>
                    <th>State</th>
                    <th>Family Friendly</th>
                    <th>Edit-Cancel Event</th>
                  </tr>
                </thead>
                <tbody>
                	<% @venues.each do |t| %>
                  <tr>
                    <th scope="row">=></th>
                    <td><%= t.name %></td>
                    <td><%= t.city %></td>
                    <td><%= t.state %></td>
                    <td><%= t.family_friendly %></td>
                    <td><%= link_to "Edit", edit_venue_path(t) %> -
                        <%= link_to "Remove", venue_path(t), method: :delete, data: { confirm: "Are you sure?" } %>
                    </td>
                  </tr>
                  <% end %>
              </tbody>
              </table>
  </div>
</div>


